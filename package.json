{
  "name": "@rainte/chrome",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --mode development && vite build -c crx/background/vite.config.ts && vite build -c crx/content/vite.config.ts && node build.js",
    "build": "tsc && vite build && vite build -c crx/background/vite.config.ts && vite build -c crx/content/vite.config.ts && node build.js",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.8.6",
    "@octokit/core": "^6.1.4",
    "@rainte/ant": "^0.0.32",
    "@rainte/js": "^0.0.33",
    "@vitejs/plugin-react": "^4.3.4",
    "antd": "^5.24.4",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-json-view": "^1.21.3",
    "react-router-dom": "^7.4.0",
    "recoil": "^0.7.7"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.310",
    "@types/node": "^22.13.12",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "^8.27.0",
    "@typescript-eslint/parser": "^8.27.0",
    "@vitejs/plugin-react-swc": "^3.8.1",
    "eslint": "^9.23.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "sass": "^1.86.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.3"
  },
  "overrides": {
    "react-json-view": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
