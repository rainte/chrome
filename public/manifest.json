{
  "manifest_version": 3,
  "name": "Rainte",
  "version": "1.0",
  "description": "Rainte Chrome CRX",
  "default_locale": "zh_CN",
  "icons": {
    "16": "/images/logo.png",
    "32": "/images/logo.png",
    "48": "/images/logo.png",
    "128": "/images/logo.png"
  },
  "action": {
    "default_icon": {
      "16": "/images/logo.png",
      "24": "/images/logo.png",
      "32": "/images/logo.png"
    },
    "default_title": "Rainte",
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "index.html",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["/images/logo.png"],
      "matches": ["<all_urls>"]
    }
  ]
}
